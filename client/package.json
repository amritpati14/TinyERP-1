    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "ej-angular2": "^15.1.33",
    "gulp": "^3.9.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^4.0.0",
    "gulp-file-contents-to-json": "^0.2.2",
    "gulp-inject": "^4.2.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-rename": "^1.2.3",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-typescript": "^3.1.6",
    "gulp-useref": "^3.1.2",
