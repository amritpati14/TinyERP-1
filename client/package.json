{
  "name": "angular2-getting-started",
  "version": "1.0.0",
  "description": "basic about angular2",
  "main": "systemjs.config.js",
  "scripts": {
    "postinstall": "typings install",
    "start": "concurrently \"npm run build-sass\" \"npm run tsc:w\" \"npm run lite\"",
    "prod": "npm run litepro",
    "litepro": "lite-server -c lite-server.prod.json",
    "lite": "lite-server",
    "tsc:w": "tsc -w",
    "tsc": "tsc",
    "build": "gulp",
    "bundle-html-templates": "gulp bundle:html-templates",
    "build-sass": "node-sass --include-path src/themes/education/resources/sass src/themes/education/resources/sass/themes/default/index.scss src/themes/education/resources/themes/default/style.css",
    "build-theme-sass": "node-sass --include-path src/themes/default/sass src/themes/default/sass/themes/default/index.scss src/themes/default/css/style.css"
  },
  "keywords": [
    "angular2",
    "getting",
    "started",
    "with",
    "angular2"
  ],
  "author": {
    "name": "TU Tran",
    "email": "contact@tranthanhtu.vn",
    "url": "http://tranthanhtu.vn"
  },
  "homepage": "http://tranthanhtu.vn",
  "license": "ISC",
