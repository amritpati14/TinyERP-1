{
  "src/modules/common/components/panel.html": "<div class=\"{{cls}}\">\r\n    <ng-container *ngIf=\"title\">\r\n        <h3 class=\"{{cls}}__title\">\r\n            {{title}}\r\n            <ng-content *ngIf=\"commandPosition==ENUMS.LayoutPosition.Top\" select=\"panel-command\"></ng-content>\r\n        </h3>\r\n    </ng-container>\r\n    <ng-content select=\"commands\"></ng-content>\r\n    <div class=\"{{cls}}__content\">\r\n        <ng-content></ng-content>\r\n    </div>\r\n    <div class=\"{{cls}}__footer\" *ngIf=\"commandPosition==ENUMS.LayoutPosition.Bottom\">\r\n        <ng-content select=\"bottom-commands\"></ng-content>\r\n    </div>\r\n</div>",
  "src/modules/common/components/panelCommand.html": "<ng-container *ngIf=\"type==ENUMS.PanelCommandType.LogOut\">\r\n    <icon-signout [text]=\"text\" (onClicked)=\"onClicked.emit($event);\"></icon-signout>\r\n</ng-container>",
  "src/modules/common/components/account/registerForm.html": "<horizontal-form [title]=\"title\">\r\n    <form-text-input *ngIf=\"mode==ENUMS.RegistrationMode.Invitation\" [labelText]=i18n.security.account.register.invitor [readOnly]=\"true\"[model]=model.invitor></form-text-input>\r\n    <form-text-input [labelText]=i18n.security.account.register.firstName [placeHolderText]=i18n.security.account.register.inputFirstName\r\n        [validation]=\"['security.account.register.validation.firstNameIsRequired']\" [(model)]=model.firstName></form-text-input>\r\n    <form-text-input [labelText]=i18n.security.account.register.lastName [placeHolderText]=i18n.security.account.register.inputLastName\r\n        [validation]=\"['security.account.register.validation.lastNameIsRequired']\" [(model)]=model.lastName></form-text-input>\r\n    <form-email-input [labelText]=i18n.security.account.register.email [placeHolderText]=i18n.security.account.register.inputEmail\r\n        [validation]=\"['security.account.register.validation.invalidEmail', 'security.account.register.validation.emailWasUsed']\"\r\n        [(model)]=model.email></form-email-input>\r\n    <form-password-input \r\n        [labelText]=i18n.security.account.register.password \r\n        [placeHolderText]=i18n.security.account.register.inputPassword\r\n        [validation]=\"['security.account.register.validation.invalidPassword', 'security.account.register.validation.passwordDoesNotMatch']\" \r\n        [(model)]=model.password>\r\n    </form-password-input>\r\n    <form-password-input \r\n        [labelText]=i18n.security.account.register.confirmPassword \r\n        [placeHolderText]=i18n.security.account.register.inputPassword\r\n        [validation]=\"['security.account.register.validation.passwordDoesNotMatch', 'security.account.register.validation.confirmPasswordWasRequired']\" \r\n        [(model)]=model.confirmPassword>\r\n    </form-password-input>\r\n\r\n    <form-primary-button (onClick)=\"onRegisterClicked($event)\" [label]=\"i18n.security.account.register.register\"></form-primary-button>\r\n    <form-button (onClick)=\"onCancelClicked($event)\" [label]=\"i18n.security.account.register.cancel\"></form-button>\r\n</horizontal-form>",
  "src/modules/common/components/auth/loginForm.html": "<horizontal-form [title]=\"'Login'\">\r\n    <form-text-input [labelText]=i18n.security.account.login.email [placeHolderText]=i18n.security.account.login.inputEmail \r\n        [validation]=\"[\r\n            'security.account.login.validation.invalidEmailOrPwd', \r\n            'security.account.login.validation.emailIsRequired',\r\n            'security.account.login.validation.accountWasNotActivated'\r\n        ]\"\r\n        [(model)]=model.userName>\r\n    </form-text-input>\r\n    <form-password-input [labelText]=i18n.security.account.login.password [placeHolderText]=i18n.security.account.login.inputPassword\r\n        [validation]=\"['security.account.login.validation.invalidEmailOrPwd', 'security.account.login.validation.pwdIsRequired']\"\r\n        [(model)]=model.password>\r\n    </form-password-input>\r\n    <form-primary-button (onClick)=\"onLoginClicked($event)\" [label]=\"i18n.common.form.save\"></form-primary-button>\r\n    <form-default-link (onClick)=\"onRegisterClicked.emit(model);\" [label]=\"i18n.security.account.login.register\"></form-default-link>\r\n</horizontal-form>",
  "src/modules/common/components/common/copyable.html": "<ng-container *ngIf=\"type==ENUMS.CopyableComponentType.Text\">\r\n    <span #content>\r\n        <ng-content></ng-content>\r\n    </span>\r\n    <icon-copy [showText]=\"true\" [text]=\"i18n.common.icons.copy\" (onClicked)=\"onCopyClicked($event)\"></icon-copy>\r\n</ng-container>",
  "src/modules/common/components/error/errorMessage.html": "<alert [cls]=\"'error-message'\" [type]=\"ENUMS.AlertType.Danger\" *ngIf=\"visible==true\" [title]=\"title\">\r\n    <list \r\n        [cls]=\"cls+'__items'\" \r\n        [itemCls]=\"cls+'__item'\"\r\n        [items]=\"errors\">\r\n        <template let-item=\"item\">\r\n            <span [innerHTML]=\"resolve(item)\"></span>\r\n        </template>\r\n    </list>\r\n</alert>",
  "src/modules/common/components/grid/grid.html": "<table id=\"{{model.id}}\" class=\"display\" width=\"100%\"></table>",
  "src/modules/common/components/icons/baseIcon.html": "<a href=\"#\" onclick=\"return false;\" title=\"{{text}}\" (click)=\"onIconClicked($event)\">\r\n    <i class=\"{{cls}}\"></i>\r\n    <ng-container *ngIf=\"showText\">\r\n        {{text}}\r\n    </ng-container>\r\n</a>",
  "src/modules/common/components/form/formButton.html": "<button class=\"btn {{cls}}\" (click)=\"onClicked($event)\" type=\"button\">{{label}}</button>",
  "src/modules/common/components/form/formDefaultLink.html": "<a onclick=\"return false;\" [ngClass]=\"cls\" class=\"btn\" onclick=\"return false;\" (click)=\"onClicked($event)\"\r\n    title=\"{{label}}\">{{label}}</a>",
  "src/modules/common/components/form/formDropdown.html": "<div class=\"form-group row\">\r\n        <label *ngIf=\"noLabel==false\" class=\"col-xs-12\" [ngClass]=\"{\r\n                    'control-label col-md-4 col-sm-4':direction==ENUMS.LayoutDirection.Horizontal, \r\n                    'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical\r\n                }\" for=\"first-name\">\r\n            {{labelText}}\r\n        </label>\r\n        <div class=\"col-xs-12\" [ngClass]=\"{\r\n                    'col-md-8 col-sm-8':direction==ENUMS.LayoutDirection.Horizontal && noLabel==false, \r\n                    'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical || noLabel==true\r\n                }\">\r\n                <select (change)=\"onValueChanged($event);\" class=\"form-control\">\r\n                    <option *ngFor=\"let item of items\" value=\"{{item.value}}\">{{item.text}}</option>\r\n                </select>\r\n        </div>\r\n    </div>",
  "src/modules/common/components/form/formDropdownButton.html": "<button data-toggle=\"dropdown\" class=\"btn btn-default dropdown-toggle\"\r\n\ttype=\"button\" aria-expanded=\"false\">\r\n        {{label}}\r\n        <span class=\"caret\"></span>\r\n    </button>\r\n<ul role=\"menu\" class=\"dropdown-menu\">\r\n\t<li *ngFor=\"let action of items\" attr.data-key=\"{{action.key}}\" (click)=\"onActionItemClicked(action)\" onclick=\"return false;\">\r\n\t\t<a href=\"#\">{{i18nHelper.resolve(action.key)}}</a>\r\n\t</li>\r\n</ul>",
  "src/modules/common/components/form/formEditor.html": "<div id=\"{{id}}\" class=\"form-group\">\r\n        <label \r\n            class=\"col-xs-12\"\r\n            [ngClass]=\"{\r\n                'control-label col-md-4 col-sm-4':direction==ENUMS.LayoutDirection.Horizontal, \r\n                'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical\r\n            }\"\r\n            for=\"first-name\">\r\n                {{labelText}}\r\n        </label>\r\n        <div \r\n            class=\"col-xs-12\"\r\n            [ngClass]=\"{'col-md-8 col-sm-8':direction==ENUMS.LayoutDirection.Horizontal, 'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical}\"\r\n        >\r\n            <p *ngIf=\"readOnly===true\">{{model}}</p>\r\n            <textarea \r\n                *ngIf=\"readOnly===false\" \r\n                rows=\"3\" \r\n                type=\"text\"\r\n                [(ngModel)]=model class=\"form-control col-md-7 col-xs-12\"></textarea>\r\n        </div>\r\n    </div>",
  "src/modules/common/components/form/formFileSelect.html": "<span class=\"{{cls}}\" onclick=\"console.log(this.children[0].click())\">\r\n        <input class=\"{{cls+'__selector'}}\" type=\"file\" accept=\"{{accept}}\" (change)=\"onChanged($event)\">\r\n</span>",
  "src/modules/common/components/form/formInput.html": "<div class=\"form-group\">\r\n    <label class=\"control-label col-md-3 col-sm-3 col-xs-12\" for=\"first-name\">{{label}}</label>\r\n    <div class=\"col-md-6 col-sm-6 col-xs-12\">\r\n        <input (change)=\"onValueChange()\" name=\"firstName\" class=\"form-control col-md-7 col-xs-12\" type=\"text\" [(ngModel)]=\"value\">\r\n    </div>\r\n</div>",
  "src/modules/common/components/form/formRadioInput.html": "<div class=\"form-check\">\r\n    <input \r\n        class=\"form-check-input\" \r\n        [disabled]=\"readOnly==true\" \r\n        [checked]=\"isSelected==true\" \r\n        (change)=\"onChanged($event)\" \r\n        type=\"{{type}}\" \r\n        id=\"ctr_{{id}}\" \r\n        name=\"{{name}}\" \r\n        [value]=\"value\">\r\n    <label class=\"form-check-label\" for=\"ctr_{{id}}\">\r\n        {{text}}\r\n    </label>\r\n</div>",
  "src/modules/common/components/form/formTextArea.html": "<div class=\"form-group\">\r\n\t<label \r\n\t\tclass=\"col-xs-12\"\r\n\t\t[ngClass]=\"{\r\n\t\t\t'control-label col-md-4 col-sm-4':direction==ENUMS.LayoutDirection.Horizontal, \r\n\t\t\t'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical\r\n\t\t}\"\r\n\t\tfor=\"first-name\">\r\n\t\t\t{{labelText}}\r\n\t</label>\r\n\t<div \r\n\t\tclass=\"col-xs-12\"\r\n\t\t[ngClass]=\"{'col-md-8 col-sm-8':direction==ENUMS.LayoutDirection.Horizontal, 'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical}\"\r\n\t>\r\n\t\t<p *ngIf=\"readOnly===true\">{{model}}</p>\r\n\t\t<textarea *ngIf=\"readOnly===false\" rows=\"3\" (change)=\"onValueChanged($event)\" placeholder=\"{{placeHolderText}}\" type=\"text\"\r\n\t\t\t[(ngModel)]=model class=\"form-control col-md-7 col-xs-12\"></textarea>\r\n\t</div>\r\n</div>",
  "src/modules/common/components/form/formTextInput.html": "<div class=\"form-group row\">\r\n\t<label *ngIf=\"noLabel==false\" class=\"col-xs-12\" [ngClass]=\"{\r\n\t\t\t\t'control-label col-md-4 col-sm-4':direction==ENUMS.LayoutDirection.Horizontal, \r\n\t\t\t\t'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical\r\n\t\t\t}\" for=\"first-name\">\r\n\t\t{{labelText}}\r\n\t</label>\r\n\t<div class=\"col-xs-12\" [ngClass]=\"{\r\n\t\t\t\t'col-md-8 col-sm-8':direction==ENUMS.LayoutDirection.Horizontal && noLabel==false, \r\n\t\t\t\t'col-md-12 col-sm-12':direction==ENUMS.LayoutDirection.Vertical || noLabel==true\r\n\t\t\t}\">\r\n\t\t<ng-container *ngIf=\"readOnly===true && copyable===true\">\r\n\t\t\t<copyable>{{model}}</copyable>\r\n\t\t</ng-container>\r\n\t\t<ng-container *ngIf=\"readOnly===true && copyable===false\">\r\n\t\t\t<p *ngIf=\"readOnly===true\">\r\n\t\t\t\t{{model}}\r\n\t\t\t</p>\r\n\t\t</ng-container>\r\n\t\t<ng-container *ngIf=\"readOnly===false\">\r\n\t\t\t<input placeholder=\"{{placeHolderText}}\" type=\"{{type}}\" [(ngModel)]=model class=\"form-control\" (blur)=\"onValueChanged($event)\" (keyup)=\"onValueChanged($event)\"\r\n\t\t\t/>\r\n\t\t</ng-container>\r\n\r\n\t</div>\r\n</div>",
  "src/modules/common/components/form/horizontalForm.html": "<form data-parsley-validate=\"\" class=\"form-horizontal form-label-left\" novalidate=\"\">\r\n    <h3 *ngIf=\"title\" class=\"text-center\">{{title}}</h3>\r\n    <!-- <ng-content select=\"form-text-input\"></ng-content>\r\n    <ng-content select=\"form-email-input\"></ng-content>\r\n    <ng-content select=\"form-textarea\"></ng-content>\r\n    <ng-content select=\"form-password-input\"></ng-content>\r\n    <ng-content select=\"form-editor\"></ng-content> -->\r\n    <ng-content></ng-content>\r\n    <div class=\"ln_solid\"></div>\r\n    <div class=\"form-group\">\r\n        <div class=\"col-md-8 col-sm-8 col-xs-12 col-sm-offset-4 col-xs-offset-0 text-left\">\r\n            <ng-content select=\"form-primary-button\"></ng-content>\r\n            <ng-content select=\"form-default-link\"></ng-content>\r\n            <ng-content select=\"form-button\"></ng-content>\r\n            <ng-content select=\"form-default-button\"></ng-content>\r\n        </div>\r\n    </div>\r\n</form>",
  "src/modules/common/components/layout/column.html": "<ng-content></ng-content>",
  "src/modules/common/components/layout/multiColumns.html": "<div class=\"container {{cls}}\">\r\n        <ng-content></ng-content>\r\n</div>",
  "src/modules/common/components/media/image.html": "<span class=\"{{cls}}\">\r\n    <img src=\"{{src}}\" alt=\"{{alt}}\" width=\"{{width}}\" height=\"{{height}}\" class=\"{{cls+'__photo'}}\" />\r\n    <form-file-select class=\"{{cls+'__editor'}}\" accept=\"{{accept}}\" (onFileSelected)=\"onNewFileSelected($event)\"></form-file-select>\r\n    <!-- <input class=\"{{cls}}__editor\" type=\"file\" accept=\"{{accept}}\" (change)=\"onNewFileSelected($event)\"> -->\r\n</span>",
  "src/modules/common/components/media/video.html": "<span class=\"{{cls}}\">\r\n    <ng-container *ngIf=\"source==Enums.VideoSourceType.File\">\r\n        <video class=\"{{cls}}__video\" width=\"100%\" height=\"{{mediaHeight}}\" controls preload=\"auto\" data-setup='{ \"playbackRates\": [1, 1.5, 2] }'\r\n            [src]=\"src\"></video>\r\n    </ng-container>\r\n    <ng-container *ngIf=\"source==Enums.VideoSourceType.Youtube\">\r\n        <iframe width=\"100%\" height=\"{{mediaHeight}}\" [src]=\"youtubeVideoUrl\" border=\"0\"></iframe>\r\n    </ng-container>\r\n    <ng-container *ngIf=\"readOnly===false\">\r\n        <form-file-select class=\"{{cls+'__editor'}}\" accept=\"{{accept}}\" (onFileSelected)=\"onNewFileSelected($event)\"></form-file-select>\r\n    </ng-container>\r\n</span>",
  "src/modules/common/components/media/videoPlayer.html": "<div class=\"{{cls}}\">\r\n    <ng-container *ngIf=\"video.source==Enums.VideoSourceType.File\">\r\n        <video class=\"{{cls}}__video video-js vjs-default-skin vjs-fluid\" width=\"100%\" height=\"{{mediaHeight}}\" controls preload=\"none\"\r\n            poster='{{options.posterUrl}}' data-setup='{ \"playbackRates\": [1, 1.5, 2] }' [src]=\"video.url\" autoplay>\r\n            <!-- <source *ngFor=\"let video of options.videos\" src=\"{{video.url}}\" type='{{video.mime}}' /> -->\r\n        </video>\r\n    </ng-container>\r\n    <ng-container *ngIf=\"video.source==Enums.VideoSourceType.Youtube\">\r\n        <iframe allow=\"autoplay; encrypted-media\" width=\"100%\" height=\"{{mediaHeight}}\" [src]=\"video.url\" border=\"0\">\r\n        </iframe>\r\n    </ng-container>\r\n</div>",
  "src/modules/common/components/notification/alert.html": "<div class=\"{{cls}}\" [ngClass]=\"{\r\n        'alert alert-danger':type==ENUMS.AlertType.Danger,\r\n        'alert alert-success':type==ENUMS.AlertType.Success,\r\n        'alert alert-warning':type==ENUMS.AlertType.Warning\r\n    }\">\r\n    <ng-content></ng-content>\r\n</div>",
  "src/modules/common/components/notification/message.html": "<ng-container *ngIf=\"type==ENUMS.MessageType.Infomation\">\r\n    <panel [cls]=\"'message--information'\" [title]=\"title\">\r\n        <content [innerHTML]=message></content>\r\n    </panel>\r\n</ng-container>\r\n<ng-container *ngIf=\"type==ENUMS.MessageType.Warning\">\r\n    <panel [cls]=\"'message--warning'\" [title]=\"title\">\r\n        <content [innerHTML]=message></content>\r\n    </panel>\r\n</ng-container>\r\n<ng-container *ngIf=\"type==ENUMS.MessageType.Error\">\r\n    <panel [cls]=\"'message--error'\" [title]=\"title\">\r\n        <content [innerHTML]=message></content>\r\n    </panel>\r\n</ng-container>",
  "src/modules/common/components/page/page.html": "<div class=\"x_panel page\">\r\n\t<div class=\"x_title\">\r\n\t\t<h3 class=\"page-title title_right\">\r\n\t\t\t<ng-content select=\"page-header\"></ng-content>\r\n\t\t</h3>\r\n\t\t<div class=\"clearfix\"></div>\r\n\t</div>\r\n\t<div class=\"page-content x_content\">\r\n\t\t<ng-content select=\"page-content\"></ng-content>\r\n\t</div>\r\n</div>",
  "src/modules/common/components/page/pageActions.html": "<div class=\"x_content\">\r\n\t<ng-container *ngFor=\"let item of model.items\">\r\n\t\t<form-button *ngIf=\"item.type!=PageActionItemType.Dropdown\" label=\"{{i18nHelper.resolve(item.key)}}\" (onClick)=item.handler($event)></form-button>\r\n\t\t<form-dropdown-button *ngIf=\"item.type==PageActionItemType.Dropdown\" label=\"{{i18nHelper.resolve(item.key)}}\" [items]=item.items></form-dropdown-button>\r\n\t</ng-container>\r\n</div>",
  "src/modules/common/components/repeater/list.html": "<ul class=\"{{cls}}\">\r\n    <li id=\"{{getItemId(item)}}\" *ngFor=\"let item of items\" class=\"{{itemCls}}\">\r\n        <ng-container [ngTemplateOutlet]=\"itemTemplate\" [ngOutletContext]=\"{item: item, onItemClicked: onItemClicked}\"></ng-container>\r\n        <!-- <ng-container [ngTemplateOutlet]=\"itemTemplate\" [ngOutletContext]=\"getContext(item)\"></ng-container> -->\r\n        <!-- <ng-template [[ngTemplateOutlet]=\"itemTemplate\" [ngTemplateOutletContext]]=\"{$implicit: item}\"></ng-template>  -->\r\n        <!-- <ng-template [ngTemplateOutlet]=\"itemTemplate\" [ngTemplateOutletContext]=\"{item: {name:item.name}}\"></ng-template> -->\r\n    </li>\r\n</ul>",
  "src/modules/common/components/tab/tab.html": "<div class=\"{{cls}}\" *ngIf=\"state==ENUMS.TabState.Active\">\r\n    <div class=\"{{cls}}__commands\" [ngClass]=\"{'container__right col-16':commandAlign==ENUMS.ItemAlign.Right}\">\r\n            <ng-content select=\"commands\"></ng-content>\r\n    </div>\r\n    <ng-content></ng-content>\r\n</div>",
  "src/modules/common/components/tab/tabs.html": "<div class=\"{{cls}}\">\r\n    <multi-columns>\r\n        <column [ngClass]=\"{'col-4': direction==ENUMS.TabDirection.Horizontal,'col-16':direction==ENUMS.TabDirection.Vertical}\">\r\n            <list [items]=\"headers\" [cls]=\"cls+'__header'\" [itemCls]=\"cls+'__header__item'\">\r\n                <template let-header=\"item\">\r\n                    <!-- will check how to remove getClsForHeaderItemLink later -->\r\n                    <form-default-link *ngIf=\"header.visible==true\" [label]=\"header.title\" [cls]=\"getClsForHeaderItemLink(header)\" \r\n                        (onClick)=\"onHeaderItemClicked(header)\"></form-default-link>\r\n\r\n                    <!-- <form-default-link [label]=\"header.text\" [cls]=\"{\r\n                        '__header__item--active__text': header.tab.state == ENUMS.TabState.Active,\r\n                        '__header__item__text': header.tab.state==ENUMS.TabState.InActive,\r\n                        '__header__item--disabled__text': header.tab.state==ENUMS.TabState.Disabled\r\n                    }\" \r\n                    (onClick)=\"onHeaderItemClicked(header)\"></form-default-link> -->\r\n                </template>\r\n            </list>\r\n        </column>\r\n        <column [ngClass]=\"{'col-12': direction==ENUMS.TabDirection.Horizontal,'col-16':direction==ENUMS.TabDirection.Vertical}\">\r\n            <div class=\"{{cls+'__content'}}\">\r\n                <ng-content select=\"tab\"></ng-content>\r\n            </div>\r\n        </column>\r\n    </multi-columns>\r\n</div>",
  "src/modules/common/components/toggle/uitoggle.html": "<a title=\"{{text}}\" (click)=\"onButtonClicked($event)\" href=\"#\" onclick=\"return false;\" class=\"{{cls}}\">\r\n    <ng-container *ngIf=\"type==ENUMS.ToggleType.PlusMinus\">\r\n        <i *ngIf=\"state!==ENUMS.ToggleState.Expand\" class=\"\" [ngClass]=\"{\r\n            'icon-collapse--white': color==ENUMS.Color.White,\r\n            'icon-collapse': color==ENUMS.Color.Default,\r\n            'icon-2x': size==ENUMS.IconSize.Large\r\n        }\"></i>\r\n        <i *ngIf=\"state==ENUMS.ToggleState.Expand\" class=\"icon-2x\" [ngClass]=\"{\r\n            'icon-expand--white': color==ENUMS.Color.White,\r\n            'icon-expand': color==ENUMS.Color.Default,\r\n            'icon-2x': size==ENUMS.IconSize.Large\r\n        }\"></i>\r\n    </ng-container>\r\n\r\n    <ng-container *ngIf=\"type==ENUMS.ToggleType.Video\">\r\n        <i *ngIf=\"state==ENUMS.VideoToggleState.Play\" class=\"icon-2x\" [ngClass]=\"{\r\n            'icon-pause--white': color==ENUMS.Color.White,\r\n            'icon-pause': color==ENUMS.Color.Default,\r\n            'icon-2x': size==ENUMS.IconSize.Large\r\n        }\"></i>\r\n        <i *ngIf=\"state!==ENUMS.VideoToggleState.Play\" class=\"icon-2x\" [ngClass]=\"{\r\n            'icon-play--white': color==ENUMS.Color.White,\r\n            'icon-play': color==ENUMS.Color.Default,\r\n            'icon-2x': size==ENUMS.IconSize.Large\r\n        }\"></i>\r\n    </ng-container>\r\n    <ng-container *ngIf=\"type==ENUMS.ToggleType.List\">\r\n        <i class=\"icon-2x\" [ngClass]=\"{\r\n            'icon-list--white': color==ENUMS.Color.White,\r\n            'icon-list': color==ENUMS.Color.Default,\r\n            'icon-2x': size==ENUMS.IconSize.Large\r\n        }\"></i>\r\n    </ng-container>\r\n    <ng-container *ngIf=\"type==ENUMS.ToggleType.Check\">\r\n            <i *ngIf=\"state==ENUMS.ToggleState.Check\" class=\"icon-check\"></i>\r\n            <i *ngIf=\"state==ENUMS.ToggleState.UnCheck\" class=\"icon-uncheck\"></i>\r\n        </ng-container>\r\n    {{text}}\r\n</a>"
}